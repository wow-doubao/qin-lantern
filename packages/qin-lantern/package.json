{
  "name": "qin-lantern",
  "type": "module",
  "version": "0.2.6-alpha.3",
  "description": "Component library encapsulating element-plus",
  "author": "wzy",
  "homepage": "https://wow-doubao.github.io/qin-lantern/",
  "repository": {
    "type": "github",
    "url": "https://github.com/wow-doubao/qin-lantern"
  },
  "keywords": [
    "element-plus",
    "component library",
    "ui",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/qin-lantern.js",
      "require": "./dist/qin-lantern.umd.cjs"
    },
    "./locale": {
      "types": "./dist/locale/index.d.ts",
      "import": "./dist/locale/index.js"
    },
    "./resolver": {
      "types": "./dist/resolver/index.d.ts",
      "import": "./dist/resolver/index.js"
    },
    "./*": "./*"
  },
  "main": "./dist/qin-lantern.umd.cjs",
  "module": "./dist/qin-lantern.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "element-plus": "^2.11.8"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vue/runtime-core": "^3.5.19",
    "@vueuse/core": "^13.7.0",
    "element-plus": "^2.11.8",
    "lodash-unified": "^1.0.3",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.19"
  },
  "devDependencies": {
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "sass": "^1.90.0",
    "unocss": "^66.4.2",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.0.6"
  }
}
